<header>
  <h1>{{ shop.name }}</h1>

  <!-- Main menu -->
  <nav>
    <ul>
      {% for link in linklists.main-menu.links -%}
        <li>
          {{ link.title | link_to: link.url }}
        </li>
      {%- endfor %}
    </ul>
  </nav>

  <form action="/search">
      <input type="search" name="q" >
      <button type="submit">Search</button>
  </form>
</header>
